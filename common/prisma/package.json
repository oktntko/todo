{
  "name": "@todo/prisma",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@10.23.0",
  "exports": {
    "./client": {
      "types": "./src/client/index.ts",
      "default": "./dist/client/index.js"
    },
    "./schema": {
      "types": "./src/schema/index.ts",
      "default": "./dist/schema/index.js"
    }
  },
  "scripts": {
    "build": "tsup-node",
    "type-check": "tsc --project tsconfig.base.json --noEmit",
    "lint": "eslint src/ --max-warnings 0",
    "format": "prettier . --write",
    "prisma:generate": "prisma generate && find src/schema -type f | xargs sed -i -e \"s#from 'zod';#from '@todo/lib/zod';#g\" && tsx src/generate.ts && prettier src/schema --write",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:seed": "tsx src/seed.ts",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/adapter-mariadb": "^7.0.0",
    "@prisma/client": "7.0.0",
    "@prisma/client-runtime-utils": "^7.0.0",
    "@todo/lib": "workspace:*"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^9.3.2",
    "@eslint/js": "^9.39.1",
    "@tsconfig/node24": "^24.0.3",
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prisma": "^7.0.0",
    "prisma-zod-generator": "^2.1.2",
    "tsup": "^8.5.1",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.47.0"
  }
}
