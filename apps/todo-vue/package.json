{
  "name": "todo-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm symlink && vite",
    "build": "pnpm symlink && run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "symlink": "run-p symlink:*",
    "symlink:@types": "ln --symbolic --force --relative --no-target-directory '../todo-express/@types' './src/@types/express'",
    "symlink:schema": "ln --symbolic --force --relative '../todo-express/src/schema' --target-directory='./src'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "run-s lint:*",
    "lint:oxlint": "oxlint src/ --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint src/ --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/list": "^6.1.17",
    "@fullcalendar/multimonth": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@fullcalendar/vue3": "^6.1.17",
    "@trpc/client": "11.4.2",
    "@vueuse/components": "^13.4.0",
    "@vueuse/core": "^13.4.0",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "i18next": "^25.2.1",
    "microdiff": "^1.5.0",
    "pinia": "^3.0.3",
    "remeda": "^2.23.1",
    "sortablejs": "^1.15.6",
    "superjson": "^2.2.2",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "zod": "^3.25.67",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.352",
    "@iconify/tailwind4": "^1.0.6",
    "@rushstack/eslint-patch": "^1.11.0",
    "@tailwindcss/vite": "^4.1.10",
    "@trpc/server": "11.4.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/express": "^5.0.3",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.3",
    "@types/sortablejs": "^1.15.8",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/runtime-core": "^3.5.17",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-plugin-oxlint": "^1.3.0",
    "eslint-plugin-vue": "^10.2.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.3.0",
    "prettier": "^3.6.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^4.1.10",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.7.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^2.2.10"
  }
}
