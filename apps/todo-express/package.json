{
  "name": "todo-express",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "packageManager": "pnpm@10.12.3",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:serve": "tsx watch --tsconfig tsconfig.node.json src/index.ts",
    "dev:type": "nodemon --watch src --exec \"pnpm type-check\" src/index.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "tsup-node",
    "type-check": "rm -rf @types && tsc --project tsconfig.type.json && tsc-alias -p tsconfig.type.json",
    "start": "node --enable-source-maps --disable-warning=ExperimentalWarning dist/index.js",
    "preview": "npm-run-all --sequential build type-check start",
    "prisma": "npm-run-all --sequential prisma:* format",
    "prisma:generate": "prisma generate && prettier src/schema/zod/ --write && find src/schema/zod -type f | xargs sed -i -e \"s/from '\\([~.]\\)\\(.*\\)';$/from '\\1\\2\\.js';/\" -e \"s@from 'zod';@from '~/lib/zod.js';@g\" ",
    "prisma:migrate-create-only": "prisma migrate dev --create-only",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "tsx prisma/seed.ts",
    "lint": "eslint src/ --fix",
    "format": "prettier src/ --write",
    "test": "vitest run",
    "test:preview": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "6.10.1",
    "@trpc/server": "11.4.2",
    "adm-zip": "^0.5.16",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.13",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "log4js": "^6.9.1",
    "multer": "2.0.1",
    "qrcode": "^1.5.4",
    "remeda": "^2.23.1",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "superjson": "^2.2.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@microsoft/api-extractor": "^7.52.8",
    "@types/adm-zip": "^0.5.7",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.3",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.3",
    "@types/qrcode": "^1.5.5",
    "@types/qs": "^6.14.0",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "nodemon": "^3.1.10",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prisma": "^6.10.1",
    "supertest": "^7.1.1",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vitest": "^3.2.4",
    "zod-prisma-types": "^3.2.4"
  }
}
