{
  "name": "todo-express",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "packageManager": "pnpm@10.12.2",
  "main": "index.js",
  "exports": {
    ".": {
      "types": "./types/router/_router.d.ts",
      "import": "./dist/index.js"
    },
    "./schema": "./src/schema/index.ts"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:serve": "tsx watch --tsconfig tsconfig.base.json src/index.ts",
    "dev:type": "nodemon --watch src --exec \"pnpm type-check\" src/index.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "tsup-node",
    "type-check": "rm -rf types && tsc --project tsconfig.base.json && tsc-alias --project tsconfig.base.json",
    "preview": "node --enable-source-maps --disable-warning=ExperimentalWarning dist/index.js",
    "lint": "eslint src/ --fix",
    "format": "prettier src/ --write",
    "test": "vitest run",
    "test:preview": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@todo/lib": "workspace:*",
    "@todo/prisma": "workspace:*",
    "@trpc/server": "11.4.3",
    "adm-zip": "^0.5.16",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "log4js": "^6.9.1",
    "multer": "2.0.1",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "superjson": "^2.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@microsoft/api-extractor": "^7.52.8",
    "@tsconfig/node24": "^24.0.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/adm-zip": "^0.5.7",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.3",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.7",
    "@types/qrcode": "^1.5.5",
    "@types/qs": "^6.14.0",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "nodemon": "^3.1.10",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "supertest": "^7.1.1",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vitest": "^3.2.4"
  }
}
