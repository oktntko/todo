{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm symlink && vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "symlink": "run-p symlink:*",
    "symlink:@types": "ln --symbolic --force --relative --no-target-directory '../express/@types' './src/@types/express'",
    "symlink:schema": "ln --symbolic --force --relative '../express/src/schema' --target-directory='./src'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint --fix src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@trpc/client": "11.0.0-rc.477",
    "@vueuse/components": "^11.0.3",
    "@vueuse/core": "^11.0.3",
    "axios": "^1.7.5",
    "dayjs": "^1.11.13",
    "deep-object-diff": "^1.1.9",
    "i18next": "^23.14.0",
    "pinia": "^2.2.2",
    "remeda": "^2.11.0",
    "sortablejs": "^1.15.3",
    "superjson": "^2.2.1",
    "vue": "^3.4.38",
    "vue-router": "^4.4.3",
    "zod": "^3.23.8",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@iconify/json": "^2.2.241",
    "@iconify/tailwind": "^1.1.3",
    "@rushstack/eslint-patch": "^1.10.4",
    "@trpc/server": "11.0.0-rc.477",
    "@tsconfig/node20": "^20.1.4",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.5.0",
    "@types/sortablejs": "^1.15.8",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/runtime-core": "^3.4.38",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "eslint-plugin-vue": "^9.27.0",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8.4.41",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.2.0",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-router": "^0.10.7",
    "vite": "^5.4.2",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^2.0.29"
  }
}
