{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm symlink && vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "symlink": "run-p symlink:*",
    "symlink:@types": "ln --symbolic --force --relative --no-target-directory '../express/@types' './src/@types/express'",
    "symlink:schema": "ln --symbolic --force --relative '../express/src/schema' --target-directory='./src'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint --fix src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/multimonth": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@fullcalendar/vue3": "^6.1.15",
    "@trpc/client": "11.0.0-rc.477",
    "@tsconfig/node22": "^22.0.0",
    "@vueuse/components": "^12.7.0",
    "@vueuse/core": "^12.7.0",
    "axios": "^1.8.1",
    "dayjs": "^1.11.13",
    "deep-object-diff": "^1.1.9",
    "i18next": "^24.2.2",
    "pinia": "^3.0.1",
    "remeda": "^2.21.0",
    "sortablejs": "^1.15.6",
    "superjson": "^2.2.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.2",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@iconify/json": "^2.2.311",
    "@iconify/tailwind": "^1.2.0",
    "@rushstack/eslint-patch": "^1.10.5",
    "@tailwindcss/vite": "^4.0.9",
    "@trpc/server": "11.0.0-rc.477",
    "@types/node": "^22.13.8",
    "@types/sortablejs": "^1.15.8",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/runtime-core": "^3.5.13",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.9",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.25.0",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.4.1",
    "unplugin-vue-router": "^0.11.2",
    "vite": "^6.2.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^2.2.6"
  }
}
