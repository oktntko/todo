{
  "name": "todo",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@10.23.0",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "turbo dev --ui=tui || true",
    "dev:msw": "MODE='msw' pnpm --filter='@todo/vue' dev:msw",
