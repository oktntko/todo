// cSpell Settings
{
  // Version of the setting file.  Always 0.2
  "version": "0.2",
  // language - current active spelling language
  "language": "en",
  // words - list of words to be always considered correct
  "words": [
    "aichat",
    "appenders",
    "arcticons",
    "autoincrement",
    "colorset",
    "Dataless",
    "dataurl",
    "daygrid",
    "daypilot",
    "dbgenerated",
    "debouncer",
    "dompurify",
    "esbuild",
    "evented",
    "filesize",
    "fontisto",
    "grayscale",
    "hugeicons",
    "iconify",
    "initdb",
    "microdiff",
    "mingcute",
    "mockopts",
    "mockreq",
    "mockres",
    "multimonth",
    "oxlint",
    "qrcode",
    "refstate",
    "remeda",
    "reqid",
    "rgba",
    "sortablejs",
    "timegrid",
    "triggerable",
    "trpc",
    "twofa",
    "unfonts",
    "unplugin",
    "userstorage",
    "vaadin",
    "vueuse"
  ],
  // flagWords - list of words to be always considered incorrect
  // This is useful for offensive words and common spelling errors.
  // For example "hte" should be "the"
  "flagWords": [],
  // Specify paths/files to ignore.
  "ignorePaths": [
    "node_modules", // this will ignore anything the node_modules directory
    ".git", // Ignore the .git directory
    "*.dll", // Ignore all .dll files.
    "pnpm-lock.yaml",
    "package.json",
    "generated",
    "dist",
    "types",
    "*.svg"
  ]
}
