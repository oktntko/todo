{
  "name": "express",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch --tsconfig tsconfig.node.json src/index.ts",
    "build": "rm -rf dist && tsc --project tsconfig.node.json && tsc-alias -p tsconfig.node.json",
    "type-check": "rm -rf @types && tsc --project tsconfig.type.json && tsc-alias -p tsconfig.node.json",
    "start": "node --disable-warning=ExperimentalWarning dist/index.js",
    "preview": "npm-run-all --sequential build type-check start",
    "prisma": "npm-run-all --sequential prisma:* format",
    "prisma:generate": "prisma generate && prettier src/schema/zod/ --write && find src/schema/zod -type f | xargs sed -i -e \"s/from '\\([~.]\\)\\(.*\\)';$/from '\\1\\2\\.js';/\" -e \"s@from 'zod';@from '~/lib/zod.js';@g\" ",
    "prisma:migrate:create-only": "prisma migrate dev --create-only",
    "prisma:deploy": "prisma migrate deploy",
    "lint": "eslint src/ --fix",
    "format": "prettier src/ --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.17.0",
    "@trpc/server": "11.0.0-rc.467",
    "axios": "^1.7.2",
    "dayjs": "^1.11.12",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "i18next": "^23.12.2",
    "log4js": "^6.9.1",
    "remeda": "^2.6.0",
    "socket.io": "^4.7.5",
    "superjson": "^2.2.1",
    "zod": "^3.23.8",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.5",
    "@types/express-session": "^1.18.0",
    "@types/qs": "^6.9.15",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prisma": "^5.17.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.18.0",
    "zod-prisma-types": "^3.1.8"
  }
}
