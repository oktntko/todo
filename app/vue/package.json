{
  "name": "@todo/vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:msw": "run-s drizzle:generate && vite --mode msw",
    "build": "vite build",
    "build:msw": "vite build --mode msw",
    "type-check": "vue-tsc --build --force tsconfig.app.json",
    "preview": "vite preview",
    "lint": "run-s lint:*",
    "lint:oxlint": "oxlint src/ --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint src/ --fix",
    "format": "prettier --write src/",
    "test:unit": "vitest run",
    "test:preview": "vitest --ui",
    "test:e2e": "playwright test",
    "drizzle:generate": "drizzle-kit generate && tsx ./drizzle.generate-migrations.ts && prettier --write src/mock/migrations.json"
  },
  "dependencies": {
    "@daypilot/daypilot-lite-javascript": "^4.8.0",
    "@todo/express": "workspace:*",
    "@todo/lib": "workspace:*",
    "@todo/prisma": "workspace:*",
    "@trpc/client": "11.7.1",
    "@vueuse/components": "^14.0.0",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.13.1",
    "dompurify": "^3.3.0",
    "fabric": "^6.7.1",
    "pinia": "^3.0.3",
    "sortablejs": "^1.15.6",
    "superjson": "^2.2.5",
    "use-vue-validate-schema": "^0.0.7",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^9.2.2",
    "@electric-sql/pglite": "^0.3.11",
    "@iconify/json": "^2.2.402",
    "@iconify/tailwind4": "^1.0.6",
    "@playwright/test": "^1.56.1",
    "@rushstack/eslint-patch": "^1.14.1",
    "@tailwindcss/vite": "^4.1.16",
    "@trpc/server": "11.7.1",
    "@tsconfig/node24": "^24.0.1",
    "@tsconfig/strictest": "^2.0.7",
    "@types/express": "^5.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.2",
    "@types/sortablejs": "^1.15.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/runtime-core": "^3.5.22",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "drizzle-kit": "^0.31.6",
    "drizzle-orm": "^0.44.7",
    "eslint": "^9.38.0",
    "eslint-plugin-oxlint": "^1.25.0",
    "eslint-plugin-vue": "^10.5.1",
    "jsdom": "^27.1.0",
    "msw": "^2.11.6",
    "msw-trpc": "^2.0.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.25.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.16",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-fonts": "^1.4.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-router": "^0.16.1",
    "vite": "^7.1.12",
    "vitest": "^4.0.6",
    "vue-tsc": "^3.1.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
