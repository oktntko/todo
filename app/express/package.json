{
  "name": "@todo/express",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "dist/index.mjs",
  "exports": {
    "./schema": "./src/schema/index.ts"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:serve": "tsx watch --tsconfig tsconfig.base.json src/index.ts",
    "dev:barrel": "tsx watch --include='src/schema' --exclude='src/schema/index.ts' ../../script/generateBarrelFile.ts src/schema",
    "build": "tsdown",
    "type-check": "rm -rf types && tsc --project tsconfig.base.json && tsc-alias --project tsconfig.base.json",
    "preview": "node --enable-source-maps dist/index.mjs",
    "lint": "eslint src/ --fix",
    "format": "prettier src/ --write",
    "test": "vitest run",
    "test:preview": "vitest --ui"
  },
  "dependencies": {
    "@todo/lib": "workspace:*",
    "@todo/prisma": "workspace:*",
    "@trpc/server": "11.8.1",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "log4js": "^6.9.1",
    "multer": "2.0.2",
    "openai": "^6.15.0",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "superjson": "^2.2.6"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.55.2",
    "@types/adm-zip": "^0.5.7",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/express-session": "^1.18.2",
    "@types/multer": "^2.0.0",
    "@types/qrcode": "^1.5.6",
    "@types/qs": "^6.14.0",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "supertest": "^7.1.4",
    "tsc-alias": "^1.8.16",
    "tsdown": "^0.18.4",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.27.0"
}
