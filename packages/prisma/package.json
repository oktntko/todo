{
  "name": "-prisma",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@10.12.2",
  "exports": {
    "./client": "./src/client.ts",
    "./schema": "./src/schema/index.ts"
  },
  "scripts": {
    "dev": "tsup src/* --watch",
    "build": "run-p type-check \"build-only {@}\" --",
    "type-check": "tsc --project tsconfig.base.json --noEmit",
    "build-only": "tsup-node",
    "lint": "eslint src/ --max-warnings 0",
    "format": "prettier . --write",
    "prisma:generate": "prisma generate && find src/schema -type f | xargs sed -i -e \"s#from 'zod';#from '-lib/zod';#g\" && prettier src/schema --write",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:seed": "tsx src/seed.ts",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "6.10.1",
    "-lib": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tsconfig/node24": "^24.0.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^24.0.4",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prisma": "^6.10.1",
    "tsup": "^8.5.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.35.0",
    "zod-prisma-types": "^3.2.4"
  }
}
