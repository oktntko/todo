{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "EXPRESS_PORT",
        "DATABASE_URL",
        "DB_PORT"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "preview": {
      "cache": false,
      "persistent": true,
      "env": [
        "EXPRESS_PORT",
        "DATABASE_URL",
        "DB_PORT"
      ]
    }
  }
}
